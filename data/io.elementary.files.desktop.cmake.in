[Desktop Entry]
_Name=Files
_Comment=Browse your files
_Keywords=folder;manager;explore;disk;filesystem;
_GenericName=File Manager
Exec=io.elementary.files %U
Icon=system-file-manager
Terminal=false
StartupNotify=true
Type=Application
MimeType=inode/directory;
Categories=System;Utility;Core;GTK;FileTools;FileManager;
X-GNOME-Gettext-Domain=io.elementary.files
X-GNOME-UsesNotifications=true
Actions=WindowNew;Root;

[Desktop Action WindowNew]
_Name=New Window
Exec=io.elementary.files -n

[Desktop Action Root]
_Name=New Window As Administrator
Exec=io.elementary.files-pkexec
