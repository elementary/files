[Desktop Entry]
Name=Files
Comment=Browse your files
Keywords=folder;manager;explore;disk;filesystem;
GenericName=File Manager
Exec=pantheon-files %U
Icon=system-file-manager
Terminal=false
StartupNotify=true
Type=Application
MimeType=inode/directory;
Categories=System;Utility;Core;GTK;FileTools;FileManager;
X-GNOME-Gettext-Domain=pantheon-files
X-GNOME-UsesNotifications=true
Actions=WindowNew;Root;AboutDialog;

[Desktop Action WindowNew]
Name=New _Window
Exec=pantheon-files -n

[Desktop Action Root]
Name=New Window As _Administrator
Exec=pantheon-files-pkexec

[Desktop Action AboutDialog]
Name=About _Files
Exec=pantheon-files --about
