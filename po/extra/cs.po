#: data/io.elementary.files.appdata.xml.in.in:36
msgid "Use new emblems for git status"
msgstr "Použity nové emblémy pro git stav"

#: data/io.elementary.files.appdata.xml.in.in:37
msgid "Show selection actions in secondary click menu"
msgstr "Zobrazeny akce výběru v kontextové nabídce (pravé tlačítko myši)"

