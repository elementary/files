#: data/files.metainfo.xml.in.in:269 data/files.metainfo.xml.in.in:289
msgid "Minor updates:"
msgstr "小型更新："

#: data/files.metainfo.xml.in.in:73 data/files.metainfo.xml.in.in:93
#: data/files.metainfo.xml.in.in:112 data/files.metainfo.xml.in.in:128
#: data/files.metainfo.xml.in.in:142 data/files.metainfo.xml.in.in:153
